<h1>Articles#show</h1>
<h2><%= @article.title %></h2>
<p><%= @article.content %></p>
<p> <%= link_to('edit article', edit_article_path) %> </p>
<p><%= link_to "Delete", article_path(@article),
      method: :delete,
      data: { confirm: "Are you sure?" } %> </p>
<p> <%= link_to('back to list', articles_path) %> </p>
